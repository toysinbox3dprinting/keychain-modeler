{
    "name": "3d-keychain-modeler",
    "version": "0.1.0",
    "homepage": "/",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.11.16",
        "@mui/material": "^5.13.7",
        "@react-three/drei": "^9.78.1",
        "@react-three/fiber": "^8.13.4",
        "@types/node": "^20.17.30",
        "@types/opentype.js": "^1.3.4",
        "@types/react": "^18.2.8",
        "@types/react-dom": "^18.2.4",
        "@types/styled-components": "^5.1.26",
        "@types/three": "^0.152.1",
        "archiver": "^6.0.0",
        "opentype.js": "^1.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "react-router-dom": "^6.11.2",
        "rimraf": "^5.0.1",
        "styled-components": "^6.0.3",
        "three": "^0.159.0",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "vite",
        "build": "npm run build:web",
        "test": "vitest run --passWithNoTests",
        "build:web": "node ./scripts/build-web.js",
        "build:prod": "node ./scripts/build-prod.js",
        "build:artifact": "node ./scripts/build-artifact.js",
        "generate:adapters": "node ./scripts/generate-geometry-adapters.js",
        "verify:adapters": "node ./scripts/verify-geometry-adapters.js",
        "typecheck": "tsc --noEmit",
        "dev": "vite",
        "preview": "vite preview",
        "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "src/core/**/*.{ts,tsx,js,jsx}"
                ],
                "rules": {
                    "no-restricted-imports": [
                        "error",
                        {
                            "patterns": [
                                "@ui/*",
                                "@infra/*",
                                "../ui/*",
                                "../../ui/*",
                                "../../../ui/*",
                                "../infra/*",
                                "../../infra/*",
                                "../../../infra/*"
                            ]
                        }
                    ]
                }
            },
            {
                "files": [
                    "src/infra/**/*.{ts,tsx,js,jsx}"
                ],
                "rules": {
                    "no-restricted-imports": [
                        "error",
                        {
                            "patterns": [
                                "@ui/*",
                                "../ui/*",
                                "../../ui/*",
                                "../../../ui/*"
                            ]
                        }
                    ]
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "esbuild": "^0.25.0",
        "eslint": "^8.57.1",
        "eslint-config-react-app": "^7.0.1",
        "vite": "^5.4.14",
        "@vitejs/plugin-react": "^4.3.4",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.8"
    }
}
